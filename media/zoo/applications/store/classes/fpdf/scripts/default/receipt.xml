					<title align="L" text-align="R" text="Order Number:" />
				</field>
				<field name="subtotal" type="textbox" x="176" y="245" h="5" w="29" align="R" font-size="10" mask="currency">
					<title align="L" text-align="R" text="Subtotal"  font-size="10"/>
				</field>
				<field name="ship_total" type="textbox" x="176" y="250" h="5" w="29" align="R" font-size="10" mask="currency">
					<title align="L" text-align="R" text="Shipping"  font-size="10"/>

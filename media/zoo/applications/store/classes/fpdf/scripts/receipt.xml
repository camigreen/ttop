<?xml version="1.0" encoding="utf-8"?>
<form name="receipt" grid="0" title="Reciept">
	<margins>
		<left>0</left>
		<right>0</right>
		<top>0</top>
		<bottom>0</bottom>
	</margins>
	<font>
		<family>arial</family>
		<style></style>
		<size>9</size>
	</font>

	<company>
		<companyname>Laportes T-Top Boat Covers</companyname>
		<address>
			<street>4651 Franchise Street</street>
			<city>North Charleston</city>
			<state>SC</state>
			<zip>29418</zip>
		</address>
		<phone>(843) 760-6101</phone>
		<website>http://www.ttopcovers.com</website>
		<email>info@ttopcovers.com</email>
		<logoPath>images/logos/ttop/TTop_100x100.png</logoPath>
	</company>
	<formats>
		<format name="item-name" font-style="B" font-size="10" />
		<format name="item-options" indent="5"/>
	</formats>
	<startPage>page1</startPage>
	<pages>
		<page name="page1" orientation="P" size="Letter">
			<fields>
				<field name="form_title" type="textbox" x="145" y="10" h="10" w="60" font-size="30" font-style="B" all-caps="1" align="R" />
				<field name="page_number" type="pageNumbers" x="175" y="25" h="5" w="30" align="R" />
				<field name="companyname" type="textbox" x="35" y="10" h="5" w="80" font-size="12" font-style="B" />
				<field name="companyaddress" type="textbox" x="35" y="15" h="35" w="80" line-height="4" />
				<field name="totals_box" type="textbox" x="156.25" y="240" h="25" w="48.75" border="B,L,R" />
				<field name="billto" type="textbox" x="10" y="45" h="30" w="75" border="1">
					<title align="T" text-align="C" border="1" font-style="B" text="Bill To" />
				</field>
				<field name="shipto" type="textbox" x="130" y="45" h="30" w="75" border="1">
					<title align="T" text-align="C" border="1" font-style="B" text="Ship To" />
				</field>
				<field name="logo" type="logo" x="10" y="10" h="25" w="25" border="0" />
				<field name="order_date" type="textbox" x="170" y="20" h="5" w="35" align="R" >
					<title align="L" text-align="R" text="Order Date:" />
				</field>
				<field name="subtotal" type="textbox" x="176" y="242" h="5" w="29" align="C" font-size="10">
					<title align="L" text-align="R" text="Subtotal"  font-size="10"/>
				</field>
				<field name="shipping" type="textbox" x="176" y="247" h="5" w="29" align="C" font-size="10">
					<title align="L" text-align="R" text="Shipping"  font-size="10"/>
				</field>
				<field name="taxes" type="textbox" x="176" y="252" h="5" w="29" align="C" font-size="10">
					<title align="L" text-align="R" text="Taxes"  font-size="10"/>
				</field>
				<field name="total" type="textbox" x="176" y="257" h="5" w="29" align="C" font-size="10">
					<title align="L" text-align="R" text="Total"  font-size="10"/>
				</field>
				<field name="order_details" type="table" x="10" y="80" rows="1" w="195">
					<columns>
						<column name="salesperson" w=".25" align="C">
							<header font-style="B" text="Salesperson" />
						</column>
						<column name="order_number" w=".25" align="C">
							<header font-style="B" text="Order Number" />
						</column>
						<column name="delivery_method" w=".25" align="C">
							<header font-style="B" text="Delivery Method" />
						</column>
						<column name="payment_information" w=".25" align="C">
							<header font-style="B" text="Payment Information" /> 
						</column>
					</columns>
				</field>
				<field name="items" type="table" x="10" y="95" rows="28" w="195">
					<columns>
						<column name="item_description" w=".75">
							<header text="Item Description" />
						</column>
						<column name="qty" w=".10" align="C">
							<header text="Qty" />
						</column>
						<column name="price" w=".15" align="C" mask="currency">
							<header text="Unit Price" />
						</column>
					</columns>
				</field>
			</fields>
			<gotopage>page2</gotopage>
		</page>
		<page name="page2" orientation="P" size="Letter">
